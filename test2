from .. import loader, utils

class TestMod(loader.Module):
    """EF mod."""
    strings = {'name': 'Test'}

    async def watcher(self, message):
        try:
            if message.chat_id in {-666607151}:
                if message.sender_id in {692657524}:     
                    if "Завершить работу" in message.message:
                        await message.chat_id in {-699031047}
                              message.respond('Завершить работу')
        except: pass
