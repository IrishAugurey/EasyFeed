from .. import loader, utils

class FarmMod(loader.Module):
    """EF mod."""
    strings = {'name': 'Farm'}

    async def watcher(self, message):
        try:
            if message.chat_id in {-1001289617428}:
                if message.sender_id in {1261343954}:      
                    if "Завершить работу" in message.message:
                        await message.respond('Завершить работу')
                    if "Откормить жабу" in message.message:
                        await message.respond('Откормить жабу')
                    if "отправиться в золотое подземелье" in message.message:
                        await message.respond('отправиться в золотое подземелье')
                    if "жабу на тусу" in message.message:
                        await message.respond('жабу на тусу')
                    if "работа грабитель" in message.message:
                        await message.respond('работа грабитель')
                    if "поход в столовую" in message.message:
                        await message.respond('поход в столовую')
                    if "Реанимировать жабку" in message.message:
                        await message.respond('Реанимировать жабку')
        except: pass
