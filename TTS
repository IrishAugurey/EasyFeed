from .. import loader, utils

class TttsMod(loader.Module):
    """EF mod."""
    strings = {'name': 'Ttts'}

    async def watcher(self, message):
        try:
            if message.chat_id in {-1001556219219, -1001520606324, -1001264854926}:
                if message.sender_id in {2036298501}:   
                   if "Откормить жабку" in message.message:
                        await message.respond('Откормить жабку')
                   if "Завершить работу" in message.message:
                        await message.respond('Завершить работу')
                   if "Отправиться в золотое подземелье" in message.message:
                        await message.respond('Отправиться в золотое подземелье')
                   if "Работа крупье" in message.message:
                        await message.respond('Работа крупье')                   
        except: pass
